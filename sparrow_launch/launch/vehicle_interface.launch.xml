<?xml version="1.0" encoding="UTF-8"?>
<launch>

  <arg name="vehicle_id" default="$(env VEHICLE_ID default)"/>
  <arg name="left_serial_port" default="/dev/ttyFixed7"/>
  <arg name="right_serial_port" default="/dev/ttyFixed6"/>
 
  <include file="$(find-pkg-share roboteq_interface)/launch/roboteq_interface.launch.xml" >
    <arg name="vehicle_id" value="$(arg vehicle_id)"/>
    <arg name="mode" value="serial"/>
    <arg name="left_serial_port" value="$(arg left_serial_port)"/>
    <arg name="right_serial_port" value="$(arg right_serial_port)"/>
  </include>

</launch>
